# Production Environment Variables for BM Parliament Testing
DJANGO_SETTINGS_MODULE=config.settings.production
DEBUG=False
DJANGO_SECRET_KEY=django-production-test-key-change-for-real-deployment
ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0

# Database (SQLite for testing, PostgreSQL for real production)
DATABASE_URL=sqlite:///db.sqlite3

# Notion API (use your actual keys)
NOTION_API_KEY=your-notion-api-key-here
NOTION_MEMBER_DATABASE=your-member-database-id
NOTION_PROGRAM_DATABASE=your-program-database-id
NOTION_REQUEST_DATABASE=your-request-database-id
NOTION_CHAPTER_DATABASE=your-chapter-database-id
NOTION_MINISTRY_DATABASE=your-ministry-database-id

# Email (console backend for testing)
EMAIL_BACKEND=django.core.mail.backends.console.EmailBackend
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_HOST_USER=your-email@gmail.com
EMAIL_HOST_PASSWORD=your-app-password
DEFAULT_FROM_EMAIL=noreply@bmparliament.gov.ph

# Security (for testing only)
SECURE_SSL_REDIRECT=False
SESSION_COOKIE_SECURE=False
CSRF_COOKIE_SECURE=False